{
  "name": "andersen-propertycross",
  "version": "1.0.0",
  "description": "study project",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --mode development --open",
    "build": "webpack --mode production",
    "lint": "eslint src/**/*.tsx",
    "stylelint": "stylelint --fix src/**/*.css",
    "lint:fix": "eslint --fix src/**/*.tsx",
    "test": "jest --watch",
    "prepush-tests": "jest"
  },
  "jest": {
    "verbose": true,
    "clearMocks": false,
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(js|jsx|ts|tsx)$",
    "setupFilesAfterEnv": [
      "<rootDir>src/setupTests.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(node_modules)/"
    ],
    "collectCoverageFrom": [
      "src/components/**/*.{js,jsx,ts,tsx}"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sss|styl)$": "<rootDir>/node_modules/jest-css-modules"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onridge/Andersen-PropertyCross.git"
  },
  "author": "Dima Stryzhakov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/onridge/Andersen-PropertyCross/issues"
  },
  "homepage": "https://github.com/onridge/Andersen-PropertyCross#readme",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react",
      "@babel/preset-typescript"
    ],
    "plugins": [
      "react-css-modules",
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run prepush-tests"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.8.3",
    "@types/jest": "^25.1.0",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "css-loader": "^3.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-typescript": "^0.14.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "jest-css-modules": "^2.1.0",
    "jest-fetch-mock": "^3.0.1",
    "postcss": "^8.2.10",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-url": "^8.0.0",
    "prettier": "^1.19.1",
    "prop-types": "^15.7.2",
    "ramda": "^0.26.1",
    "redux-thunk": "^2.3.0",
    "require": "^2.4.20",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.0.0",
    "stylelint": "^12.0.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.5",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "dependencies": {
    "babel-plugin-react-css-modules": "^5.2.6",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "reset-css": "^5.0.1"
  }
}
